<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/header') %>
    <body>        
        <div class="container mt-2"></div>
            <div>
                <header>
                    <%- include('../partials/nav') %>
                    <div>
                        <h1>Welcome to Mr Coffee's Scheduling app</h1>  
                    </div>                
                </header>              
            </div>
        </div>            
        <main>
            <div>
                <br>
                <p><strong>Full Name: <%= users[0].firstname %> <%= users[0].lastname %></strong></p>
                <p><strong>Email: </strong><%= users[0].email %></p>
                 <% users.forEach(user => { %>                  
                    <p><strong>Day: </strong><%= user.day %></p>
                    <p><strong>Start_at: </strong><%= user.start_at %></p>
                    <p><strong>End_at: </strong><%= user.end_at %></p>
                    <% }) %>              
            </div>                
        </main>
		<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js" integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s" crossorigin="anonymous"></script>
    </body>    
</html>